VANGUARD_ACCOUNT_NUMBERS=

# Webull
# WEBULL="WEBULL_USERNAME:WEBULL_PASSWORD:WEBULL_DID:WEBULL_TRADING_PIN"
WEBULL=

# Wells Fargo
